{
  "rewrites": [
    {
      "source": "/api/v1/auth/google/callback",
      "destination": "https://eventx.up.railway.app/api/v1/auth/google/callback"
    },
    {
      "source": "/api/(.*)",
      "destination": "https://eventx.up.railway.app/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}
